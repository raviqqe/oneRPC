{
  "name": "onerpc",
  "version": "0.4.24",
  "description": "The RPC library for the serverless and TypeScript era.",
  "repository": "https://github.com/raviqqe/onerpc",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "main": "dist/main.js",
  "typesVersions": {
    "*": {
      "client.js": [
        "dist/client.d.ts"
      ],
      "middleware.js": [
        "dist/middleware.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./client.js": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./middleware.js": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./adapter/*": {
      "types": "./dist/adapter/*.d.ts",
      "default": "./dist/adapter/*.js"
    },
    "./adapter/*.js": {
      "types": "./dist/adapter/*.d.ts",
      "default": "./dist/adapter/*.js"
    }
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --bundle --minify --format=esm --packages=external --sourcemap --outdir=dist src/*.ts src/adapter/*.ts",
    "build:tsc": "tsc --emitDeclarationOnly",
    "lint": "eslint src",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@raviqqe/hidash": "^2.0.0",
    "@raviqqe/loscore": "^0.2.0"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^2.0.2",
    "@raviqqe/tsconfig": "^1.0.1",
    "@types/aws-lambda": "^8.10.130",
    "@vitest/coverage-v8": "^1.0.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.34.0",
    "typescript": "^5.0.2",
    "vitest": "^1.0.1"
  },
  "peerDependencies": {
    "valibot": "^0.24.1",
    "zod": "^3.22.4"
  },
  "workspaces": [".", "examples/aws"]
}
