{
  "name": "onerpc",
  "version": "0.4.18",
  "description": "The RPC library for the serverless and TypeScript era.",
  "repository": "https://github.com/raviqqe/onerpc",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "main": "dist/main.js",
  "typesVersions": {
    "*": {
      "client.js": [
        "dist/client.d.ts"
      ],
      "middleware.js": [
        "dist/middleware.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./client.js": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    },
    "./middleware.js": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    }
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --bundle --minify --format=esm --packages=external --sourcemap --outdir=dist src/*.ts",
    "build:tsc": "tsc --emitDeclarationOnly",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@raviqqe/hidash": "^1.6.2",
    "valibot": "^0.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^1.2.10",
    "@raviqqe/tsconfig": "^1.0.1",
    "@vitest/coverage-v8": "^0.34.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.34.0",
    "typescript": "^5.0.2",
    "vitest": "^0.34.1"
  }
}
